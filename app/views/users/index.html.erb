<h1>Listing Users</h1>
<%= link_to 'New user', new_user_path %>
<table>
  <tr>
    <th>User Name</th>
    <th>Password</th>
    <th colspan="3"></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.user_name %></td>
      <td><%= user.password %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>